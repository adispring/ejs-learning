<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <% for(var i=0;i<10;i++){ %>
            <%= i %>
        <% } %>
        <!-- 获取变量 -->
        <div class="datas">
            <p>获取变量：</p>
            <%- title %>
            <%= title %>
        </div>

        <ul>
          <% users = [
            { 'name': 'barney',  'age': 36, 'active': true },
            { 'name': 'fred',    'age': 40, 'active': false },
            { 'name': 'pebbles', 'age': 1,  'active': true }
          ] %>
          <% users.forEach(function(user){ %>
            <%- include('view', {user: user}); %>
          <% }); %>
        </ul>
    </body>
</html>
